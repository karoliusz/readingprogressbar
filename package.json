  "bugs": {
    "url": "https://github.com/karoliusz/readingprogressbar/issues"
  },
  "homepage": "https://github.com/karoliusz/readingprogressbar#readme",
  "dependencies": {
    "@swc/helpers": "^0.3.8",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.4.1",
    "@parcel/transformer-sass": "^2.4.1",
    "@parcel/transformer-typescript-types": "^2.4.1",
    "@tsconfig/recommended": "^1.0.1",
    "parcel": "^2.4.1",
    "typescript": "^4.6.3"
