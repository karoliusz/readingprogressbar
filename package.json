{
  "name": "readingprogressbar",
  "version": "0.0.1",
  "description": "Simple library for visualizing the progress of reading for a blog posts or other vertically aligned content.",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "browserslist": "defaults",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "demo": "parcel serve --target demo",
    "build": "parcel build --target css --target js"
  },
  "targets": {
    "css": {
      "source": "src/scss/main.scss",
      "distDir": "dist/css"
    },
    "js": {
      "source": "src/index.ts",
      "distDir": "dist/js"
    },
    "demo": {
      "source": "demo/demo.html",
      "distDir": "dist/demo"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karoliusz/readingprogressbar.git"
  },
  "author": "Karol Bereza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karoliusz/readingprogressbar/issues"
  },
  "homepage": "https://github.com/karoliusz/readingprogressbar#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "parcel": "^2.3.2"
  },
  "dependencies": {
    "rxjs": "^7.5.5"
  }
}
